{
  "name": "starter-hosted",
  "description": "A starter project for hosted solutions with Express, Vue, Webpack, Babel and Unrest",
  "author": "Evan Verworn <evan@verworn.net>",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --inline --hot",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "watch": "supervisor -e js,hbs,html -i ./server/client,./server/public ./server/bin/www",
    "start": "node ./server/bin/www"
  },
  "engine": {
    "node": ">4.0.0"
  },
  "dependencies": {
    "body-parser": "~1.15.0",
    "cookie-parser": "~1.4.1",
    "debug": "~2.2.0",
    "express": "~4.13.4",
    "hbs": "~4.0.0",
    "less-middleware": "2.1.x",
    "morgan": "~1.7.0",
    "nosql": "^3.1.0",
    "serve-favicon": "~2.3.0",
    "unrest": "^2.1.1",
    "vue": "^1.0.18"
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "babel-runtime": "^6.6.1",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "less": "^2.6.1",
    "less-loader": "^2.2.2",
    "url-loader": "^0.5.7",
    "vue-hot-reload-api": "^1.3.2",
    "vue-html-loader": "^1.2.0",
    "vue-loader": "^8.2.1",
    "vue-router": "^0.7.11",
    "vue-style-loader": "^1.0.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
